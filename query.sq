PREFIX  agvoc: <https://data.agrimetrics.co.uk/def/> 
PREFIX  owl:  <http://www.w3.org/2002/07/owl#> 
PREFIX  xsd:  <http://www.w3.org/2001/XMLSchema#> 
PREFIX  skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX  rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
PREFIX  hsecls: <https://data.agrimetrics.co.uk/datasets/hse/def/classes/> 
PREFIX  rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
PREFIX  hse:  <https://data.agrimetrics.co.uk/datasets/hse/def/> 
select * where {
    BIND (<https://data.agrimetrics.co.uk/datasets/hse/data/AgriProduct/K0SB5IQHINS8DMML3RINOTCQKIF5CDIJ> as ?target)
    ?target hse:activeIngredient ?targetIngredient .
    ?targetIngredient hse:concentration ?targetConcentration; 
        hse:unit ?targetUnit; 
        agvoc:activeIngredient ?targetAI .
    ?i a hsecls:Ingredient ; 
        hse:concentration ?targetConcentration;
        hse:unit ?targetUnit;
         agvoc:activeIngredient ?targetAI .
    ?product hse:activeIngredient ?i .
} LIMIT 10
